{
  "name": "wallet-inspector",
  "version": "0.1.0",
  "main": "dist/src/index.js",
  "bin": {
    "searchor": "./dist/src/index.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "start": "node dist/src/index.js",
    "test": "jest",
    "dev": "npm run clean && npm run build && node dist/src/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push --force-reset",
    "db:seed": "tsx src/database/example.ts",
    "version": "node scripts/version.js",
    "postversion": "git push && git push --tags"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "ethereum",
    "wallet",
    "analysis",
    "blockchain",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "description": "A powerful command-line tool for deep analysis of Ethereum addresses",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/commander": "^2.12.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "@types/ora": "^3.1.0",
    "jest": "^29.7.0",
    "prisma-kysely": "^1.8.0",
    "ts-jest": "^29.3.2",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@envio-dev/hypersync-client": "^0.6.3",
    "@prisma/client": "^6.9.0",
    "axios": "^1.8.4",
    "better-sqlite3": "^11.10.0",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.5.0",
    "ethers": "^6.13.5",
    "inquirer": "^12.6.1",
    "kysely": "^0.28.2",
    "open": "^9.1.0",
    "ora": "^8.2.0",
    "prisma": "^6.9.0",
    "puppeteer": "^24.11.2",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "winston": "^3.11.0",
    "xlsx-js-style": "^1.2.0",
    "yaml": "^2.8.0",
    "zod-prisma-types": "^3.2.4"
  }
}
